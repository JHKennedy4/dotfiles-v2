[push]
	default = simple
[user]
	name = John Kennedy
	email = jhk@bn.co
[color]
	ui = true
[merge]
	tool = kdiff3
[core]
	excludesfile = /Users/jkennedy/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
